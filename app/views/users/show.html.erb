<%= render partial: 'users/list', locals: { book: @book , user: @user } %>

<div class="col-md-7">
<h2>Books</h2>
<% @books.each do |book| %>
<table class="table table-striped">
<tr>
<td><%= image_tag current_user.profile_image_id.url %></td>
<td>Title</td>
<td><%= link_to book.title, book_path(book.id) %></td>
<td>Opinion</td>
<td><%= book.body %></td>
</tr><br>
</table>
<% end %>
</div>
            
<%= render partial: 'users/list', locals: { book: @book , user: @user } %>

<div class="col-md-8 offset-md-1">
<% @users.each do |user| %>
<tr>
<th><%= attachment_image_tag user, :profile_image, size: "100x100", fallback: "default_image.jpg" %></th>
<th><%= user.name %></th>
<th><%= link_to "Show", user_path(user.id) %></th>
</tr><br>
<% end %>
</div>
